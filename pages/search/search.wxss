page {
  background: #fff;
}
.search {
  display: flex;
  padding: 14rpx 0 14rpx 30rpx;
  /* background: #f5f5f5; */
}
.search-main {
  flex: 1;
  display: flex;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 30rpx;
}
.search-img {
  width: 30rpx;
  height: 30rpx;
  margin: 15rpx 14rpx 15rpx 20rpx;
}
.search-input {
  flex: 1;
  height: 60rpx;
  font-size: 14px;
}
.search-text {
  width: 110rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 32rpx;
  color: #5f6575;
}


.tag-main {
  display: flex;
  flex-wrap: wrap;
  min-height: 100%;
  padding: 20rpx 30rpx;
}
.tag {
  height: 50rpx;
  padding: 0 22rpx;
  margin: 0 30rpx 20rpx 0;
  line-height: 50rpx;
  border-radius: 25rpx;
  border: 1px solid #ddd;
}